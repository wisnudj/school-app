<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%=title%></title>
  </head>
  <body>
    <h1><%=title%></h1>
    <form action="/teacher/edit/<%=dataTeacher[0].id%>" method="post">
      <input type="input" name="first_name" value="<%=dataTeacher[0].first_name%>">
      <input type="input" name="last_name" value="<%=dataTeacher[0].last_name%>">
      <input type="input" name="email" value="<%=dataTeacher[0].email%>">
      <select name="SubjectId">
        <%for(var i = 0; i < dataSubject.length; i++) {%>
        <option value="<%=dataSubject[i].id%>"><%=dataSubject[i].subject_name%></option>
        <%}%>
      </select>
      <input type="submit" value="submit">
    </form>
  </body>
</html>
