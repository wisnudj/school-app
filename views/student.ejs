<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Students</title>
  </head>
  <body>
    <h1>Student</h1>
    <a href="/student/add">add Students</a>
    <a href="/">Home</a>
    <a href="teacher">Teachers</a>
    <a href="subject">Subjects</a>
    <a href="student">Students</a>
    <table>
      <tr>
        <th>id</th>
        <th>first_name</th>
        <th>last_name</th>
        <th>email</th>
      </tr>
      <%for(var i = 0; i < dataStudent.length; i++) {%>
        <tr>
          <td><%=dataStudent[i].id%></td>
          <td><%=dataStudent[i].first_name%></td>
          <td><%=dataStudent[i].last_name%></td>
          <td><%=dataStudent[i].email%></td>
          <td><a href="/student/edit/<%=dataStudent[i].id%>">edit</a></td>
          <td><a href="/student/delete/<%=dataStudent[i].id%>">delete</a></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
