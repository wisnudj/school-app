<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%=title%></title>
  </head>
  <body>
    <h1><%=title%></h1>
    <h3><a href="/subject/add">Add Subject</a></h3>
    <% include ./partial/menu%>
    <table>
      <tr>
        <th>id</th>
        <th>subject_name</th>
        <th>teacher</th>
        <th>action</th>
      </tr>
      <%dataSubject.forEach((element) => {%>
      <tr>
        <td><%=element.id%></td>
        <td><%=element.subject_name%></td>
        <td>
          <ol>
            <%for(var i = 0; i < element.Teachers.length; i++) {%>
            <li><%=element.Teachers[i].first_name%> <%=element.Teachers[i].last_name%></li>
            <%}%>
          </ol>
        </td>
        <td><a href="/subject/<%=element.id%>/enrolledstudent">Enrolled Student</a></td>
      </tr>
      <%})%>
    </table>
  </body>
</html>
